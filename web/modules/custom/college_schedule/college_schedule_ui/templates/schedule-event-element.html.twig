{#
/**
 * @file
 * Default theme implementation for the marquee render element.
 *
 * Available variables:
 * - attributes: Attributes for the marguee tag.
 * - content: The text to display within the marquee tag.
 *
 * @ingroup themeable
 * @ingroup render_example
 */
#}

<div{{ attributes }}>
  <div class="event-label">{{ label }}</div>
  {% if subgroup %}<div class="event-subgroup">({{ subgroup }}) </div>{% endif %}
  {% if location %}<div class="event-location">{{ location }}</div>{% endif %}
</div>
